{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "parser": "babel-eslint",
    "globals": {
        "document": true
    },
    "rules": {
        "default-case": 0,
        "react/jsx-filename-extension": 0,
        "react/no-array-index-key": 0,
        "react/forbid-prop-types": 0,
        "react/require-default-props": 0,
        "react/no-danger": 0
    }
}
